{
  "version": 2,
  "builds": [
    {
      "src": "build_web.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*\\.(js|json|wasm|png|jpg|jpeg|svg|css|ico|ttf|otf|woff|woff2))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}